{
    "name": "react-admin-ts",
    "version": "1.0.0",
    "description": "react 后台管理系统",
    "main": "webpack-config.js",
    "scripts": {
        "dev": "cross-env NODE_ENV=development webpack serve --config ./scripts/webpack.dev",
        "build": "cross-env NODE_ENV=production webpack --config scripts/webpack.prod",
        "lint": "eslint src --fix --ext .ts,.tsx,.js,.jsx "
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "npm run eslint",
            "prettier .prettierrc.js --write",
            "git add"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "ie >= 9",
        "not op_mini all"
    ],
    "dependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "classnames": "^2.3.1",
        "cross-env": "^7.0.3",
        "prettier": "^2.3.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^5.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-transform-runtime": "^7.14.5",
        "@babel/preset-env": "^7.12.11",
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "@babel/runtime-corejs3": "^7.14.7",
        "@types/axios": "^0.14.0",
        "@types/copy-webpack-plugin": "^8.0.1",
        "@types/css-minimizer-webpack-plugin": "^3.0.2",
        "@types/mini-css-extract-plugin": "^2.0.1",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.7",
        "@types/react-router-config": "^5.0.2",
        "@types/react-router-dom": "^5.1.7",
        "@types/terser-webpack-plugin": "^5.0.4",
        "@typescript-eslint/eslint-plugin": "^4.28.3",
        "@typescript-eslint/parser": "^4.11.0",
        "babel-loader": "^8.2.2",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "clean-webpack-plugin": "^3.0.0",
        "copy-webpack-plugin": "^9.0.0",
        "css-loader": "^5.0.1",
        "css-minimizer-webpack-plugin": "^3.0.1",
        "cssnano": "^4.1.10",
        "eslint": "^7.16.0",
        "eslint-plugin-html": "^6.1.2",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react": "^7.24.0",
        "eslint-webpack-plugin": "^2.4.1",
        "fork-ts-checker-webpack-plugin": "^6.2.10",
        "html-webpack-plugin": "^5.3.2",
        "mini-css-extract-plugin": "^1.3.3",
        "node-sass": "^5.0.0",
        "postcss-flexbugs-fixes": "^5.0.2",
        "postcss-loader": "^4.1.0",
        "postcss-preset-env": "^6.7.0",
        "sass-loader": "^10.1.0",
        "style-loader": "^2.0.0",
        "typescript": "^4.1.3",
        "webpack": "^5.11.0",
        "webpack-cli": "^4.3.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-merge": "^5.8.0",
        "webpackbar": "^5.0.0-3"
    }
}
